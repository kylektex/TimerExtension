
{
  "manifest_version": 2,

  "name": "Timer Extension",
  "description": "This extension will record the amount of time spent on each website",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com*" ],
      "js": ["content.js"]
    }
  ],

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}
